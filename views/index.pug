doctype html
html
	head
		meta(charset='UTF-8')
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport", content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no")
		title= title
	body
		div
			span server-text:
			span(style="color: #0275d8; font-weight: bold;") #{txt}
		div
			span client text&nbsp;
			span#client-text(style="font-weight: bold;")
		script(src='/dist/build.js')
